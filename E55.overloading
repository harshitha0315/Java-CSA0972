import java.util.*;
class Adder{  
	static int add(int a,int b)
	{	
		return a+b;	
	}  
	static double add(double a,double b)	
	{
		return a+b;
	}
	static float add(float a,float b)
	{
		return a+b;
	}  
}  
class testoverloading
{  
	public static void main(String[] args)
	{  
		System.out.println(Adder.add(3,12));
		System.out.println(Adder.add(15.2,3.9));
		System.out.println(Adder.add(15.3,11.3)); 
		 
	}
} 
